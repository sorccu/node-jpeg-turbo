{
  "name": "@julusian/jpeg-turbo",
  "version": "0.0.2",
  "description": "Limited libjpeg-turbo bindings for Node.js.",
  "keywords": [
    "jpeg",
    "jpeg-turbo",
    "libjpeg-turbo"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/julusian/node-jpeg-turbo.git"
  },
  "main": "./index.js",
  "types": "./module.d.ts",
  "dependencies": {
    "bindings": "^1.5.0",
    "cmake-js": "^5.2.0",
    "nan": "^2.14.0",
    "prebuild-install": "^5.3.0"
  },
  "scripts": {
    "install": "prebuild-install || cmake-js compile --target jpegturbo",
    "prebuild": "prebuild --all --verbose --backend cmake-js -- --target jpegturbo",
    "prebuild-upload": "prebuild --upload-all --backend cmake-js -- --target jpegturbo"
  },
  "devDependencies": {
    "prebuild": "^8.2.1"
  }
}
